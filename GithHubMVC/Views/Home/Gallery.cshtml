
@model IEnumerable<GithHubMVC.Models.ItemModel>
@{ ViewBag.Title = "Gallery"; }

<div class="row">
    <div class="text-center">
        <ul class="list-inline">
            <!--<li>@Html.ActionLink("Cameras", "ItemModel", "Home", new { id = 1 }, null)</li>-->
            <li>@Html.ActionLink("Default", "ItemModel", "Home", new { id = 0 }, null)</li>
        </ul>
    </div>
</div>
<div class="row-fluid">
    <div class="span2">
        <div>
            @foreach (var repo in Model)
            {
                <div class="photo-div">
                    <img alt="@repo.RepositoryName" src="@Url.Content(@repo.Path)" class="photo" onclick="display(this);" />
                </div>
            }
        </div>
    </div>
</div>
<div id="divimage" class="modal">
    <img class="modal-content" id="modalimg">
    <div id="caption"></div>
</div>



