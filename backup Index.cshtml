@model IEnumerable<GithHubMVC.Models.ItemModel>


ResponseModel
@{
    ViewBag.Title = "Home Page";
}


<@Page Language="C#" EnableSessionState="True">

  
    <div class="row">
        <div class="text-center">
            <ul class="list-inline">
                <li>@Html.ActionLink("Default", "", "Home", null, null)</li>
                <li>@Html.ActionLink("Main", "", "Main", null, null)</li>

                <li>@Html.ActionLink("SessionSave", "SessionSave", "Home", new { RepositoryName = "Rona's repo", Path = "https://avatars1.githubusercontent.com/u/22239854?v=4" }, null)</li>
            </ul>
        </div>
    </div>
    <div class="row-fluid">
        <div class="span2">
            <div>
                @foreach (var repo in Model)
                {
                    <p><a href="http://localhost:52125/Home/SessionSave?RepositoryName=rona&Path=htts" class="btn btn-primary btn-lg">Bookmark</a></p>
                    <p><a href="http://localhost:52125/Home/GetSessionData" class="btn btn-primary btn-lg">GetSessionData</a></p>

                    <p><a href="http://localhost:52125/Home/Search?RepositoryName=YOUR_SEARCH_KEYWORD" class="btn btn-primary btn-lg">Search</a></p>
                    <p><a href="http://localhost:52125/Home/Search1?RepositoryName=YOUR_SEARCH_KEYWORD" class="btn btn-primary btn-lg">Search1</a></p>
                    <p><a href="http://localhost:52125/Home/Search2?RepositoryName=YOUR_SEARCH_KEYWORD" class="btn btn-primary btn-lg">Search2</a></p>


                    <h3>@repo.RepositoryName</h3>
                    <div class="photo-div">
                        <img alt="@repo.RepositoryName" src="@Url.Content(@repo.Path)" class="photo" onclick="display(this);" />

                    </div>
                    <br />
                }
            </div>
        </div>
    </div>



    <div id="divimage" class="modal">
        <img class="modal-content" id="modalimg">
        <div id="caption"></div>
    </div>





